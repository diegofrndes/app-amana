	<queryString>
		<![CDATA[SELECT adiantamento.data as dataadiantamento, SUM(adiantamento.valor),count(*) FROM adiantamento
WHERE adiantamento.forma <>
"CRÉDITO ACUMULADO"
AND adiantamento.data >= $P{DATA_INICIAL} AND adiantamento.data <= $P{DATA_FINAL}
GROUP BY day(adiantamento.data) ORDER BY day(adiantamento.data)]]>
	</queryString>
